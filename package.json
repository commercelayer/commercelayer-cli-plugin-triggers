{
  "name": "@commercelayer/cli-plugin-triggers",
  "description": "Commerce Layer CLI Triggers plugin",
  "version": "1.0.5",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bin": {
    "cl-triggers": "./bin/run"
  },
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-triggers/issues",
  "devDependencies": {
    "@oclif/dev-cli": "git+https://github.com/pviti/dev-cli.git",
    "@oclif/plugin-help": "^3.2.10",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.22",
    "@types/inquirer": "^8.1.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.x",
    "@types/update-notifier": "^5.1.0",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10.0.2",
    "inflector-js": "^1.0.1",
    "jsonapi-typescript": "^0.1.3",
    "lodash": "^4.17.21",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-triggers",
  "keywords": [
    "oclif",
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-triggers",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "triggers": {
        "description": "Execute resource triggers",
        "hidden": true
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-triggers",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && npm run version",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "readme": "oclif-dev readme",
    "generate": "ts-node gen/generator.ts"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@commercelayer/js-auth": "^2.2.4",
    "@commercelayer/sdk": "^2.3.10",
    "@oclif/command": "^1.8.4",
    "@oclif/config": "^1.17.1",
    "chalk": "^4.1.2",
    "cli-ux": "^5.6.3",
    "inquirer": "^8.2.0",
    "tslib": "^2.3.1",
    "update-notifier": "^5.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
